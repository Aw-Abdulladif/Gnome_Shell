[library]
namespace = "Shell"
version = "@SHELL_VERSION@"
browse_url = "https://gitlab.gnome.org/GNOME/gnome-shell/"
repository_url = "https://gitlab.gnome.org/GNOME/gnome-shell.git"
website_url = "https://www.gnome.org"
license = "GPL-2.0-or-later"
description = "Shell"
dependencies = [ "GObject-2.0", "Shell-1.0" ]
devhelp = true
search_index = true

  [dependencies."GObject-2.0"]
  name = "GObject"
  description = "The base type system library"
  docs_url = "https://developer.gnome.org/gobject/stable"

  [dependencies."Pango-1.0"]
  name = "Pango"
  description = "Text shaping and rendering"
  docs_url = "https://docs.gtk.org/Pango/"

[theme]
name = "basic"
show_index_summary = true

[source-location]
base_url = "https://gitlab.gnome.org/GNOME/gnome-shell/-/blob/HEAD/"

[extra]
urlmap_file = "shell-urlmap.js"
